{
  "name": "fzf-import-cli",
  "version": "1.0.0",
  "description": "CLI tool for finding and importing JavaScript/TypeScript modules",
  "main": "dist/index.js",
  "bin": {
    "fzf-import": "dist/cli.js"
  },
  "scripts": {
